{
  "dataset_reader": {
    "name": "amazon_ecommerce_reader",
    "data_path": "amazon_ecommerce",
    "catalog": "Phones"
  },
  "dataset_iterator": {
    "name": "data_learning_iterator"
  },
 "chainer": {
    "in": ["x", "a", "b"],
    "out": ["y"],
    "pipe": [
      {
        "name": "ecommerce_bot",
        "in": ["x", "a", "b"],
        "fit_on": ["x"],
        "min_similarity": 0.5,
        "min_entropy": 0.5,
        "save_path": "ecommerce_bot/ecommerce_Phones_model.pkl",
        "load_path": "ecommerce_bot/ecommerce_Phones_model.pkl",
        "tokenizer": {
          "name": "stream_spacy_tokenizer"
        },
        "preprocess": {
          "name": "ecommerce_preprocess"
        },
        "out": ["y"]
      }
    ]
   },
   "metadata": {
    "labels": {
      "telegram_utils": "RankingEmbModel",
      "server_utils": "RankingEmbModel"
    },
    "requirements": [
        "../dp_requirements/spacy.txt"
      ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/embeddings/glove.6B.100d.txt",
        "subdir": "embeddings"
      }
    ]
  }
}
