{
  "chainer": {
    "in": [
      "sentences"
    ],
    "pipe": [
      {
        "in": ["sentences"],
        "name": "lazy_tokenizer",
        "out": ["tokens"]
      },
      {
        "name": "elmo_embedder",
        "elmo_output_names": ["lstm_outputs1", "lstm_outputs2", "word_emb"],
        "mini_batch_size": 32,
        "in": [
          "tokens"
        ],
        "spec": "http://files.deeppavlov.ai/deeppavlov_data/elmo_ru-twitter_2013-01_2018-04_600k_steps.tar.gz",
        "out": [
          "tokens_emb"
        ]
      }
    ],
    "out": [
      "tokens_emb"
    ]
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf.txt",
      "../dp_requirements/elmo.txt"
    ]
  }
}
