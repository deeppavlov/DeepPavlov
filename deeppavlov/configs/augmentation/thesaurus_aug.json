{
  "chainer": {
    "in": [
      "x"
    ],
    "pipe": [
      {
        "config_path": "{CONFIGS_PATH}/morpho_tagger/UD2.0/morpho_ru_syntagrus_pymorphy.json",
        "in": [
          "x"
        ],
        "out": [
          "morpho_tags"
        ]
      },
      {
        "in": [
          "x"
        ],
        "class_name": "lazy_tokenizer",
        "out": [
          "x_tokens"
        ]
      },
      {
        "in": [
          "x_tokens",
          "morpho_tags"
        ],
        "out": [
          "variants"
        ],
        "class_name": "thesaurus_aug",
        "lang": "rus",
        "penalty_for_source_token": 0.95,
        "isalpha_only": true,
        "ru_thes_dirpath": "{DOWNLOADS_PATH}/ruthes_lite2/",
        "replace_freq": 1.0,
        "with_source_token": true,
        "force_inflect": true,
        "not_replaced_tokens": ["нравится", "любить", "блин", "быть"],
        "replaced_pos_tags": ["NOUN", "VERB", "ADV", "ADJ"]
      },
      {
        "in": "variants",
        "out": "aug_x_tok",
        "class_name": "deeppavlov.models.spelling_correction.electors.kenlm_elector:KenlmElector",
        "load_path": "{MODELS_PATH}/lms/ru_wiyalen_no_punkt.arpa.binary"
      },
      {
        "in": "aug_x_tok",
        "out": "aug_x",
        "class_name": "ru_tokenizer"
      }
    ],
    "out": [
      "aug_x"
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs"
    }
  }
}
